{
  "name": "vis",
  "version": "0.0.0",
  "license": "mit",
  "author": {
    "name": "Brett Neese",
    "email": "brett@neese.rocks"
  },
  "scripts": {
    "prepublish": "jspm bundle src/main.js app-bundle.js --minify",
    "start": "./node_modules/.bin/live-server . ----entry-file=index.html"
  },
  "jspm": {
    "main": "main.js",
    "directories": {
      "lib": "src",
      "dist": "dist",
      "baseURL": "www"
    },
    "configFiles": {
      "jspm": "jspm.config.js"
    },
    "dependencies": {
      "bluebird": "npm:bluebird@3.4.0",
      "browser-request": "npm:browser-request@^0.3.3",
      "jquery": "npm:jquery@^2.2.4",
      "page": "npm:page@^1.7.1",
      "underscore": "npm:underscore@^1.8.3",
      "uri": "github:medialize/URI.js@^1.18.1"
    },
    "devDependencies": {
      "typescript": "npm:typescript@^1.6.2"
    },
    "peerDependencies": {
      "os": "github:jspm/nodelibs-os@^0.2.0-alpha",
      "process": "github:jspm/nodelibs-process@^0.2.0-alpha"
    },
    "overrides": {
      "npm:bluebird@3.4.0": {
        "map": {
          "./js/release/bluebird.js": {
            "browser": "./js/browser/bluebird.js"
          }
        },
        "meta": {
          "js/browser/bluebird.js": {
            "format": "global"
          },
          "js/browser/bluebird.min.js": {
            "format": "global"
          }
        }
      },
      "npm:jquery@2.2.4": {
        "format": "amd"
      },
      "npm:typescript@1.8.10": {
        "browser": {},
        "map": {
          "buffer": "@empty",
          "child_process": "@empty",
          "fs": "@empty",
          "path": "@empty",
          "process": "@empty",
          "readline": "@empty"
        }
      }
    }
  },
  "dependencies": {
    "connect": "^3.4.1",
    "connect-history-api-fallback": "^1.2.0",
    "express": "^4.13.4",
    "express-history-api-fallback": "^2.0.0",
    "live-server": "^1.0.0",
    "spa-express-static-server": "0.0.12"
  }
}
